package Database;

public class Osoby 
{
	private String Meno;
	private int Rok_nar;
	public int Count_uvaz;
	public static int Max_uvaz = 1;
	
	Osoby(String meno, int rok_nar)
	{
		this.Meno = meno;
		this.Rok_nar = rok_nar;
	}

	public String getMeno() {
		return Meno;
	}

	public void setMeno(String meno) {
		if(meno instanceof String)
		{
			Meno = meno;
		}
		else
		{
			System.out.println("Enter string type to set name please");
		}
	}

	public int getRok_nar() {
		return Rok_nar;
	}

	public void setRok_nar(int rok_nar) {
		if(rok_nar instanceof int)
		{
			Rok_nar = rok_nar;
		}
		else
		{
			System.out.println("Enter int type to set birth year please");
		}
		
	}
	
}
